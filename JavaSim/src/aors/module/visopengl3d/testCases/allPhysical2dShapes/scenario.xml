                    <DisplayInfo content="test13"/>
                </PhysicalObjectView>
                
                <PhysicalObjectView physicalObjectIdRef="14">
                    <PhysicalShape2D>
                        <Arc startAngle="0" endAngle="270" texture="backgrounds\\Sunflower.jpg"/>
                    </PhysicalShape2D>
                    <DisplayInfo content="test14"/>
                </PhysicalObjectView>
                
