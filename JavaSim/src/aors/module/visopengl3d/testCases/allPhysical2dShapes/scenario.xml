                    <DisplayInfo content="test7"/>
                </PhysicalObjectView>
                
                <PhysicalObjectView physicalObjectIdRef="8">
                    <PhysicalShape2D>
                        <Square texture="backgrounds\\Sunflower.jpg"/>
                    </PhysicalShape2D>
                    <DisplayInfo content="test8"/>
                </PhysicalObjectView>
                
                <PhysicalObjectView physicalObjectIdRef="9">
                    <PhysicalShape2D>
