    </xs:choice>
  </xs:group>
  <xs:group name="AgentControlUI.GUI.BlockElements.multilingual">
    <xs:sequence>
      <xs:element name="multilingualContent" maxOccurs="unbounded">
        <xs:complexType mixed="true">
          <xs:sequence>
            <xs:group ref="aors:AgentControlUI.GUI.BlockElements.monolingual" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
          <xs:attributeGroup ref="aors:lang_mandatory"/>
        </xs:complexType>
      </xs:element>          
    </xs:sequence>
  </xs:group>
  <xs:group name="AgentControlUI.GUI.BlockElements">
    <xs:choice>
      <xs:group ref="aors:AgentControlUI.GUI.BlockTextElements"/>
    	<!--
      <xs:group ref="aors:AgentControlUI.GUI.TableElements"/>
      <xs:group ref="aors:AgentControlUI.GUI.FormElements"/
    	-->
    </xs:choice>
  </xs:group>
 
 <!-- block text -->
  
  <xs:group name="AgentControlUI.GUI.BlockTextElements.monolingual">
    <xs:choice>
      <xs:element name="div" type="aors:AgentControlUI.GUI.FlowContent.monolingual.type"/>
