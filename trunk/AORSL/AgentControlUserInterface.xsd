        <xs:complexType mixed="true">
          <xs:sequence>
            <xs:group ref="aors:AgentControlUI.GUI.InlineElements.monolingual" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
          <xs:attributeGroup ref="aors:lang_mandatory"/>
        </xs:complexType>
      </xs:element>          
    </xs:sequence>
  </xs:group>
  <xs:group name="AgentControlUI.GUI.InlineElements">
    <xs:choice>
      <xs:group ref="aors:AgentControlUI.GUI.InlineElements-label"/>
      <xs:group ref="aors:AgentControlUI.GUI.LabelElements"/>
    </xs:choice>
  </xs:group>
  
  <!-- inline elements - label -->

  <xs:complexType name="AgentControlUI.GUI.InlineContent-label.monolingual.type">
    <xs:complexContent mixed="true">
      <xs:extension base="aors:AgentControlUI.GUI.EmptyContent.type">
        <xs:sequence>
          <xs:group ref="aors:AgentControlUI.GUI.InlineElements-label.monolingual" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AgentControlUI.GUI.InlineContent-label.type">
    <xs:complexContent>
      <xs:extension base="aors:AgentControlUI.GUI.EmptyContent.type">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:group ref="aors:AgentControlUI.GUI.InlineElements-label.multilingual"/>
          <xs:group ref="aors:AgentControlUI.GUI.InlineElements-label"/>
        </xs:choice>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:group name="AgentControlUI.GUI.InlineElements-label.monolingual">
    <xs:choice>
      <xs:group ref="aors:AgentControlUI.GUI.InlineTextElements.monolingual"/>
      <xs:group ref="aors:AgentControlUI.GUI.ImageElements.monolingual"/>
      <xs:group ref="aors:AgentControlUI.GUI.InlineFormElements.monolingual"/>
    </xs:choice>
  </xs:group>
  <xs:group name="AgentControlUI.GUI.InlineElements-label.multilingual">
    <xs:sequence>
      <xs:element name="multilingualContent" maxOccurs="unbounded">
        <xs:complexType mixed="true">
          <xs:sequence>
            <xs:group ref="aors:AgentControlUI.GUI.InlineElements-label.monolingual" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
          <xs:attributeGroup ref="aors:lang_mandatory"/>
        </xs:complexType>
      </xs:element>          
    </xs:sequence>
  </xs:group>
  <xs:group name="AgentControlUI.GUI.InlineElements-label">
    <xs:choice>
      <xs:group ref="aors:AgentControlUI.GUI.InlineTextElements"/>
      <xs:group ref="aors:AgentControlUI.GUI.ImageElements"/>
      <xs:group ref="aors:AgentControlUI.GUI.InlineFormElements"/>
    </xs:choice>
  </xs:group>
  
  <!-- inline text -->
  
  <xs:group name="AgentControlUI.GUI.InlineTextElements.monolingual">
    <xs:choice>
      <xs:element name="br" type="aors:AgentControlUI.GUI.EmptyContent.type"/>
      <xs:element name="code" type="aors:AgentControlUI.GUI.InlineContent.monolingual.type"/>
      <xs:element name="em" type="aors:AgentControlUI.GUI.InlineContent.monolingual.type"/>
    	<xs:element name="strong" type="aors:AgentControlUI.GUI.InlineContent.monolingual.type"/>
    	<xs:element name="span" type="aors:AgentControlUI.GUI.InlineContent.monolingual.type"/>
    </xs:choice>
  </xs:group>
  <xs:group name="AgentControlUI.GUI.InlineTextElements">
    <xs:choice>
      <xs:element name="br" type="aors:AgentControlUI.GUI.EmptyContent.type"/>
      <xs:element name="code" type="aors:AgentControlUI.GUI.InlineContent.type"/>
      <xs:element name="em" type="aors:AgentControlUI.GUI.InlineContent.type"/>
    	<xs:element name="strong" type="aors:AgentControlUI.GUI.InlineContent.type"/>
    	<xs:element name="span" type="aors:AgentControlUI.GUI.InlineContent.type"/>
    </xs:choice>
  </xs:group>
  
  <!-- image -->

  <xs:group name="AgentControlUI.GUI.ImageElements.monolingual">
    <xs:sequence>
      <xs:element name="img">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="aors:AgentControlUI.GUI.EmptyContent.type">
              <xs:attribute name="alt" type="xs:string" use="required"/>
              <xs:attribute name="src" type="xs:anyURI" use="required"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="AgentControlUI.GUI.ImageElements">
    <xs:sequence>
      <xs:element name="img">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="aors:AgentControlUI.GUI.EmptyContent.type">
              <xs:sequence>
                <xs:element name="alt" type="aors:AgentControlUI.GUI.TextContent.multilingual.type" maxOccurs="unbounded"/>
              </xs:sequence>
              <xs:attribute name="src" type="xs:anyURI" use="required"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  
  <!-- block -->
  
  <xs:complexType name="AgentControlUI.GUI.BlockContent.monolingual.type">
    <xs:complexContent mixed="true">
      <xs:extension base="aors:AgentControlUI.GUI.EmptyContent.type">
        <xs:sequence>
          <xs:group ref="aors:AgentControlUI.GUI.BlockElements.monolingual" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AgentControlUI.GUI.BlockContent.type">
    <xs:complexContent>
      <xs:extension base="aors:AgentControlUI.GUI.EmptyContent.type">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:group ref="aors:AgentControlUI.GUI.BlockElements.multilingual"/>
          <xs:group ref="aors:AgentControlUI.GUI.BlockElements"/>
        </xs:choice>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
    
  <xs:group name="AgentControlUI.GUI.BlockElements.monolingual">
    <xs:choice>
      <xs:group ref="aors:AgentControlUI.GUI.BlockTextElements.monolingual"/>
    	<!--
      <xs:group ref="aors:AgentControlUI.GUI.TableElements.monolingual"/>
      <xs:group ref="aors:AgentControlUI.GUI.FormElements.monolingual"/>
