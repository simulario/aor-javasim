      <xs:element name="pre" type="aors:AgentControlUI.GUI.InlineContent.monolingual.type"/>
    </xs:choice>
  </xs:group>
  <xs:group name="AgentControlUI.GUI.BlockTextElements">
    <xs:choice>
      <xs:element name="div" type="aors:AgentControlUI.GUI.FlowContent.type"/>
      <xs:element name="p" type="aors:AgentControlUI.GUI.InlineContent.type"/>
      <xs:element name="pre" type="aors:AgentControlUI.GUI.InlineContent.type"/>
    </xs:choice>
  </xs:group>
	
