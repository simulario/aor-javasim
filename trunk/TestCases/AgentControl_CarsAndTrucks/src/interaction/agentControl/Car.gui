<?xml-stylesheet type="text/css" href="Car.css"?><Structure>
							<h1>Properties</h1>
							<ul>
								<li>
									<label>speedLimit</label>
									<textfield slot="speedLimit" readonly="true" type="Integer"></textfield>
								</li>
								<li>
									<label>maxVelocity</label>
									<textfield slot="maxVelocity" readonly="true" type="Integer"></textfield>
								</li>
								<li>
									<label>myVelocity</label>
									<textfield slot="myVelocity" readonly="true" type="Integer"></textfield>
								</li>
							</ul>
							<h1>Actions</h1>
							<ul>
								<li>
									<div>
										<label>modifySpeedBy</label>
										<slider slot="__speed" minValue="0" maxValue="10" initialValue="5" type="Integer"></slider>
										<textfield slot="__speed" initialValue="5" type="Integer" name="speed"></textfield>
									</div>
									<div>
										<button title="speedUp" name="SpeedUp"></button>
										<button title="slowDown" name="SlowDown"></button>
									</div>
								</li>
							</ul>
            </Structure>